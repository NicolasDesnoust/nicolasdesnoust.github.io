{
  "name": "frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm task front:build"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm task front:deploy"]
      }
    }
  }
}
